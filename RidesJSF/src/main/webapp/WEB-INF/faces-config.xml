<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>main</managed-bean-name>
		<managed-bean-class>eredua.bean.MainBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>QueryRides</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>HomeDriver</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>CreateRides</from-outcome>
			<to-view-id>/CreateRides.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRides.xhtml</display-name>
		<from-view-id>/CreateRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Home</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Driver</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Close</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Login</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Admin</from-outcome>
			<to-view-id>/MainAdmin.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainAdmin.xhtml</display-name>
		<from-view-id>/MainAdmin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Close</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Traveler</from-outcome>
			<to-view-id>/MainTraveler.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainTraveler.xhtml</display-name>
		<from-view-id>/MainTraveler.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Close</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainAdmin.xhtml</display-name>
		<from-view-id>/MainAdmin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>QueryRides</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainTraveler.xhtml</display-name>
		<from-view-id>/MainTraveler.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>QueryRides</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>HomeAdmin</from-outcome>
			<to-view-id>/MainAdmin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryRides.xhtml</display-name>
		<from-view-id>/QueryRides.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>HomeTraveler</from-outcome>
			<to-view-id>/MainTraveler.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>CreateCar</from-outcome>
			<to-view-id>/CreateCars.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateCars.xhtml</display-name>
		<from-view-id>/CreateCars.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Home</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
